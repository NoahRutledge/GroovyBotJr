{
	"name": "GroovyBotJr.",
	"version": "1.1.0",
	"description": "A music playing bot",
	"scripts": {
		"start": "npm run build && node -r tsconfig-paths/register dist/groovy",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"prettier": "prettier --write **/*.{ts,js,json,yml,yaml}",
		"build": "tsc",
		"build:check": "tsc --noEmit --incremental false"
	},
	"author": "Noah R",
	"dependencies": {
		"@discordjs/voice": "^0.7.0",
		"@discordjs/opus": "^0.7.0",
		"discord.js": "^13.0.0",
		"libsodium-wrappers": "^0.7.9",
		"youtube-dl-exec": "1.2.4",
		"ytdl-core": "^4.9.1",
		"ytsr": "^3.5.3"
	},
	"overrides": {
		"discord-api-types": "^0.30.0"
	},
	"devDependencies": {
		"tsconfig-paths": "^3.9.0",
		"typescript": "~4.2.2"
	}
}